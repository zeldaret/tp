CompileFlags:
  Add: [-Wno-c++11-compat-deprecated-writable-strings, -Wno-extern-initializer, -Wno-multichar]
---
If:
  PathMatch: .*/*.inc
Diagnostics:
  Suppress:
    - "undeclared_var_use"
    - "undeclared_var_use_suggest"
    - "bound_member_function"
    - "typecheck_subscript_value"
    - "unknown_typename"
